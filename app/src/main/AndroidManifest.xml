<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.softlaboratory.restohub">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Restaurant">
        <activity
            android:name="com.softlaboratory.restohub.Register"
            android:exported="false" />
        <activity
            android:name="com.softlaboratory.restohub.Login"
            android:exported="false" />
        <activity
            android:name="com.softlaboratory.restohub.Splash"
            android:exported="true"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.softlaboratory.restohub.MainActivity"
            android:exported="false" />
    </application>

</manifest>